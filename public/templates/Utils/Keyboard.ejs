<div class="custom-keyboard">

    <div class="text-keyboard flex flex-column">
        <div class="keyboard-style"></div>

        <% var lines = [
            [81, 87, 69, 82, 84, 89, 85, 73, 79, 80, 219],
            [65, 83, 68, 70, 71, 72, 74, 75, 76, 186, 222],
            [90, 88, 67, 86, 66, 78, 77, 188, 190, {key: 8, title: 'remove', class: 'key-remove'}],
            [
                {key: 1346, title: 'hide', class: 'key-hide'},
                {key: 32, class: 'key-space'},
                {key: 13, title: 'enter', class: 'key-enter'},
            ]

        ];
         var indexKey = 1;
        %>

        <div class="flex-auto autocompete-keyboard"></div>

        <% for (var i in lines) { %>
            <div class="flex-auto flex keys-line">

                <% for (var j in lines[i]) { var keycode = lines[i][j] %>

                    <% if (typeof keycode == 'object') { %>


                        <div class="flex-auto key-button <%=keycode.class ? keycode.class : ''%>"
                             data-key="<%=keycode.key%>"
                             on-touchstart="onKeyTouchStart"
                             on-touchend="onKeyTouchEnd"
                                >

                            <% if (keycode.title) { %>
                                <div class="key-title"><%= keycode.title %></div>
                            <% } %>
                        </div>


                    <% } else { %>

                        <div class="flex-auto key-button"
                             on-touchstart="onKeyTouchStart"
                             on-touchend="onKeyTouchEnd" data-key="<%=keycode%>">
                            <div class="key-title"><%= KeyBoard.keyCodes[keycode][indexKey] %></div>
                        </div>
                    <% } %>


                <% }  %>

            </div>
        <% }  %>

        <div class="flex-auto autocompete-keyboard"></div>
    </div>

</div>